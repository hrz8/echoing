{
  "name": "@echoing/logger",
  "version": "0.0.0",
  "description": "logger package for echoing app",
  "author": "hirzinurfakhrian@gmail.com",
  "type": "module",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.5.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "package.json"
  ],
  "scripts": {
    "build": "tsc --p tsconfig.json",
    "dev": "concurrently --kill-others --success all --names build \"tsc --watch --p tsconfig.json\"",
    "check-types": "tsc --noEmit",
    "lint": "echo 'unimplemented'"
  },
  "dependencies": {
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@echoing/eslintconfig": "workspace:*",
    "@echoing/tsconfig": "workspace:*",
    "@types/node": "^22.10.2",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.9",
    "typescript": "^5.7.2"
  }
}
